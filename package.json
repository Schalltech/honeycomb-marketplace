{
  "name": "@schalltech/honeycomb-marketplace",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "author": "Eric Schall",
  "private": true,
  "scripts": {
    "start": "start-storybook -p 3000 -c .storybook",
    "bootstrap": "lerna link convert && npm install && npm run install:hooks",
    "install:hooks": "node scripts/install-hooks.js",
    "create:micro-app": "cd micro-apps && npx @schalltech/honeycomb-cli@latest create-micro-app",
    "build:micro-apps": "lerna exec --concurrency 10 --no-bail --scope ma-* -- node ../../scripts/build-micro-apps.js",
    "build:storybook": "build-storybook -c .storybook -o ./docs",
    "host:marketplace": "lerna run --scope app-local-marketplace start",
    "clean": "npm run clean:artifacts && npm run clean:packages &&  npm run clean:root",
    "clean:artifacts": "lerna run clean --parallel",
    "clean:packages": "lerna clean --yes",
    "clean:root": "rimraf node_modules",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "test:quick": "lerna run test:quick --concurrency=1 --stream",
    "test:coverageLerna": "lerna run test:coverage --parallel",
    "test:watchLerna": "lerna run test:watch --parallel",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json",
    "lint": "eslint -c .eslintrc.js --ignore-path .eslintignore --ext js,jsx ./micro-apps/ ./applications/",
    "lint:fix": "eslint --fix -c .eslintrc.js --ignore-path .eslintignore --ext js,jsx ./micro-apps/ ./applications/",
    "commit": "npx git-cz",
    "prepare": "lerna exec --parallel --scope @schalltech/* -- npm i"
  },
  "workspaces": [
    "applications/*",
    "libraries/*",
    "micro-apps/**"
  ],
  "dependencies": {
    "@schalltech/honeycomb-cli": "file:libraries\\honeycomb-cli",
    "app-local-marketplace": "file:applications\\app-local-marketplace",
    "ma-button": "file:micro-apps\\button",
    "ma-image": "file:micro-apps\\image"
  },
